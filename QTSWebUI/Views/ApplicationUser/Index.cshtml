@model IEnumerable<ApplicationUser>

@{
    ViewBag.Title = "Users";
}

<h2>@ViewBag.Title </h2>

<p>
    @Html.ActionLink("Create New", "Create", null,new {@class = "btn btn-success btn-xs"})
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
        <td>
            @Html.ActionLink("Delete", "Delete", new { id=item.Id }, new {@class="btn btn-danger btn-xs", onclick="return confirm('Delete selected user?');"})
        </td>
    </tr>
}

</table>
